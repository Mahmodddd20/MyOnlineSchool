  }).catch(error=>{\n            // history.push('/login')\n        })\n\n    }\n    function fetchClass(){\n        api.myclass(props.match.params.id).then(\n            response => {\n                setClassName(response.data.name);\n            }).catch(error=>{\n            history.push('/login')\n        })\n\n    }\n\n\n\n    function emptyWeeks(){\n        return(\n            <Spinner3 delay=\"5000\"/>\n            );\n    }\n\n\n    function allWeeks(){\n        return week.map(week=>{\n            return(\n                <Card key={week.id} className='mt-2 mb-2 ml-0 mr-2 w-100'>\n                    <Card.Header as=\"h5\" className='overflow-auto p-4 flex-column align-middle h-100'>\n                        {week.name}\n                        {CookieService.get('role')=='teacher'?\n                            <div className='d-block  float-right align-middle '>\n                                <Button variant=\"danger\" className='float-right ml-1' onClick={()=>{{handleShow(week.id)}}}>Delete</Button>\n                                <Button variant=\"warning\" className='float-right mr-1' href={'/week/edit/' + week.id}>Edit</Button>\n                            </div>\n                            :''}\n                    </Card.Header>\n                    <Card.Body>\n                        <Card.Title>From {week.start_date} To {week.end_date}</Card.Title>\n                        <Card.Text>\n                            {week.description}.\n                        </Card.Text>\n                        <Col className='m-0 p-0'>\n                            {CookieService.get('role')=='teacher'?\n                                <div className='m-0'>\n                                <Button className='ml-0 mt-2' variant=\"outline-success\" href={'/newmaterial/'+week.id}>Add Materials</Button><br/>\n                                <Button className='ml-0 mt-2' variant=\"outline-success\" href={'/newhomework/'+week.id}>Add Homeworks</Button>\n                                </div>:''}\n                            {CookieService.get('role')!=='teacher'?\n                                <div>\n                                    <Button className='ml-0 mt-2' variant=\"primary\" href={\"/materials/all/\"+week.id}>View Materials</Button><br/>\n                                <Button className='ml-0 mt-2' variant=\"primary\" href={\"/homeworks/all/\"+week.id}>View Homeworks</Button>\n                                </div>:''}\n                            <Button className='ml-0 mt-2' variant=\"primary\" href={\"/week/show/\"+week.id}>View {week.name}</Button>\n                        </Col>\n                    </Card.Body>\n                </Card>\n            );\n        })\n    }\n\n\n\n    return(\n        <Container fluid className='text-capitalize m-2 ml-5' >\n            <Row>\n                <Col xs='auto' md='auto' lg=\"10\" className='ml-0 pl-0'>\n                <h1 className='m-2'>\n                    <Badge pill variant=\"success\" className='rounded-0 text-wrap'>\n                    {className==''?\n                        <span className=\"spinner-border spinner-border-sm spinner-grow\" role=\"status\" aria-hidden=\"true\"></span>\n                        :className}\n                    </Badge>\n                </h1>\n                </Col>\n                <Col xs md lg=\"1\" >\n                    <Sidebar/>\n                </Col>\n\n            </Row>\n            <Row>\n                {CookieService.get('role')=='teacher'?<Button className='m-2' variant=\"outline-primary\" href={'/neweek/'+props.match.params.id}>create new week</Button>:''}\n                {CookieService.get('role')=='admin'?<Button className='m-2' variant=\"info\" href={'/addstudenttoclass/'+props.match.params.id}>add students to the class</Button>:''}\n                {CookieService.get('role')!=='student'?<Button className='m-2' variant=\"outline-info\" href={'/sendemail/'+props.match.params.id}>Send Email To Students</Button>:''}\n                <Button variant='outline-dark' className='float-right m-2' onClick={()=>history.goBack()}>Go Back</Button>\n\n            </Row>\n            <Row className='w-75 overflow-auto mb-5'>\n                <CardColumns className=' ml-2'>\n\n                    {week.length >0 ? allWeeks() : emptyWeeks() }\n                </CardColumns>\n            </Row>\n\n            <>\n                <Modal\n                    show={show}\n                    onHide={handleClose}\n                    backdrop=\"static\"\n                    keyboard={false}\n                    centered\n                >\n                    <Modal.Header closeButton>\n                        <Modal.Title>Confirm</Modal.Title>\n                    </Modal.Header>\n                    <Modal.Body>\n                        Are you sure you want to Delete the week.\n                    </Modal.Body>\n                    <Modal.Footer>\n                        <Button variant=\"info\" onClick={handleClose}>\n                            No\n                        </Button>\n                        <Button variant=\"danger\" onClick={()=>{deleteWeek(del) ;handleClose()}}>Yes Delete</Button>\n                    </Modal.Footer>\n                </Modal>\n            </>\n\n\n        </Container>\n\n\n    )\n}","/home/acer/Videos/school/frontend/src/components/Homework/allhomeworks.js",["215","216","217","218"],"import React, {useState,useEffect} from \"react\";\nimport api from \"../../api\";\nimport { Link ,useHistory } from 'react-router-dom';\nimport {Badge, Button, Card, CardColumns, CardGroup, Col, Container, Row, Tab, Tabs} from \"react-bootstrap\";\nimport Spinner from \"../Loading/Spinner\";\nimport Sidebar from \"../Sidebar/sidebar\";\n\n\nexport default function AllHomeworks(props){\n    const [homework, setHomework] =useState([]);\n    const [key, setKey] = useState('');\n\n\n    const history = useHistory();\n    useEffect(() => {\n        fetchHomeworks();\n    },[]);\n\n\n    function fetchHomeworks(){\n        api.myhomeworks(props.match.params.id).then(response=>{\n            setHomework(response.data)\n        }).catch(error=>{\n            history.push('/login');\n        })\n    }\n\n    function renderHomeworks(){\n        return( homework.map(homework=> {\n                return(\n                    <Tab tabClassName='ml-0 mt-2 text-capitalize text-monospace btn-outline-primary mr-2 ' eventKey={homework.id} title={homework.name}>\n                        <Card key={homework.id} className='ml-0 mt-2 align-items-center w-75 mb-5'>\n                            <Card.Body className='ml-0'>\n                                <Card.Text dangerouslySetInnerHTML={{__html: homework.description}}>\n                                </Card.Text>\n                                <Button className='m-2 w-100' variant=\"outline-info\" href={\"/homework/show/\"+homework.id}>\n                                    Enter {homework.name}</Button>\n\n                            </Card.Body>\n                        </Card>\n                    </Tab>\n                );\n            })\n        );\n    }\n\n    function emptyHomeworks(){\n        return(\n               <Spinner delay=\"5000\"/>\n\n        )\n    }\n\n    return (\n        <Container className='ml-5 m-2 text-capitalize  '>\n            {homework.length>0?<>\n                <Row>\n                    <Col xs='auto' md='auto' lg=\"10\" className='ml-0 pl-0'>\n                    <h1 className='ml-3 m-2'>\n                        <Badge pill variant=\"success\" className='rounded-0 text-wrap'>\n                            welcome to your homeworks\n                        </Badge>\n                    </h1>\n                    </Col>\n                    <Col xs md lg=\"1\" >\n                        <Sidebar/>\n                    </Col>\n\n                </Row>\n                <Row>\n                    <Button variant='outline-dark' className='ml-3 m-2' onClick={()=>h